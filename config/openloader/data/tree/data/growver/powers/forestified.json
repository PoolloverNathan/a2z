{
	"name": "Â§aForestified",
	"description": "You have become a tree, able to photosynthesize, but also getting the expected downside of being immobile. (if you die you'll be freed)",
	"type": "origins:multiple",

	"rooted": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:motion 0 @s"
		},
		"interval": 2
	},

	"unseen": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:visibility 0 @s"
		}
	},

	"burying": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:delay",
			"ticks": 20,
			"action":{
			"type": "origins:execute_command",
            "command": "tp @s ~ ~-0.5 ~"
		}
		}
	},

	"unburying": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
            "command": "tp @s ~ ~0.5 ~"
		}
	},

	"hardbark": {
		"type": "origins:modify_damage_taken",
		"damage_condition": {
			"type": "origins:name",
			"name": "on_fire",
			"inverted": true
		},
		"modifier": {
			"name": "wooden bastion",
			"operation": "multiply_total_multiplicative",
			"value": -0.8
		}
	},

	"burningwood": {
		"type": "origins:modify_damage_taken",
		"damage_condition": {
			"type": "origins:name",
			"name": "on_fire"
		},
		"modifier": {
			"name": "wooden achiles",
			"operation": "multiply_total_multiplicative",
			"value": 1.5
		}
	},

	"waterimmunity": {
		"type": "origins:invulnerability",
		"damage_condition": {
			"type": "origins:name",
			"name": "hurt_by_water"
		}
	},

	"doublerooted": {
		"type": "eggolib:prevent_key_use",
		"keys": [
			"key.sprint",
			"key.jump",
			"key.forward",
			"key.back",
			"key.left",
			"key.right"
		]
	},
	"nostomach": {
		"type": "origins:prevent_item_use",
		"item_condition": {
		  "type": "origins:food"
		}
	},

	"photosynth": {
		"type": "origins:action_over_time",
		"entity_action": {
		  "type": "origins:feed",
		  "food": 1,
		  "saturation": 1
		},
		"interval": 220,
		"condition": {
		  "type": "origins:and",
		  "conditions": [
			{
			  "type": "origins:exposed_to_sky"
			},
			{
			  "type": "origins:daytime"
			}
		  ]
		}
	  },
	  "nobreaking": {
		"type": "origins:modify_break_speed",
		"modifier": {
			"operation": "multiply_total_multiplicative",
			"value": -1
		}
	  },
	"deathremoval": {
		"type": "origins:action_on_death",
		"bientity_action": {
			"type": "origins:target_action",
			"action": {
			"type": "origins:execute_command",
			"command": "revoke power @s growver:forestified"
		}
		}
	},
	"movereset": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:motion"
		}
	},

	"seereset": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:visibility"
		}
	},

	"growresource": {
		"type": "origins:resource",
		"min": 0,
		"max": 6
	},

	"growing": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:change_resource",
			"resource": "growver:forestified_growresource",
			"change": 1
		},
		"interval": 1799
	},

	"grow": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "scale multiply pehkui:base 1.2 @s",
			"condition": {
				"type": "origins:resource",
				"resource": "growver:forestified_growresource",
				"comparison": "==",
				"compare_to": 6,
				"inverted": true
			}
		},
		"interval": 1800
	},

	"shrink1": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command":"scale divide pehkui:base 1.2 @s"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 1
		}
	},

	"shrink2": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command":"scale divide pehkui:base 1.2 @s"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 2
		}
	},

	"shrink3": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command":"scale divide pehkui:base 1.2 @s"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 3
		}
	},

	"shrink4": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command":"scale divide pehkui:base 1.2 @s"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 4
		}
	},

	"shrink5": {
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command":"scale divide pehkui:base 1.2 @s"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 5
		}
	},

	"noridingorelse": {
		"type": "origins:prevent_entity_use"
	},

	"noelytra": {
		"type": "origins:prevent_elytra_use"
	},

	"nosleep": {
		"type": "origins:prevent_sleep"
	},

	"nodrown": {
			"type": "origins:invulnerability",
			"damage_condition": {
				"type": "origins:name",
				"name": "drown"
			}
	},

	"rootsight": {
		"type": "origins:entity_glow",
		"condition": {
			"type": "origins:resource",
			"resource": "growver:forestified_growresource",
			"comparison": ">=",
			"compare_to": 3
		},
		"red": 0.0,
		"green": 1.0,
		"blue": 0.0
	}
}