{
    "type": "origins:action_over_time",
    "entity_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "effect give @s regeneration 10 1"
            },
            {
                "type": "origins:execute_command",
                "command": "effect give @s resistance 10"
            },
            {
                "type": "origins:delay",
                "ticks": 10,
                "action": {
                    "type": "origins:execute_command",
                    "command": "power remove @s growver:give"
                }
            }
        ]
    },
    "interval": 6,
    "condition": {
        "type": "origins:power",
        "power": "growver:blacklisted",
        "inverted": true
    }
}