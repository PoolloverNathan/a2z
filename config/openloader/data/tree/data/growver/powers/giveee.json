{
    "type": "origins:action_over_time",
    "entity_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "summon minecraft:experience_orb ~ ~ ~ {Value:10}"
            },
            {
                "type": "origins:delay",
                "ticks": 10,
                "action": {
                    "type": "origins:execute_command",
                    "command": "power remove @s growver:giveee"
                }
            }
        ]
    },
    "interval": 7,
    "condition": {
        "type": "origins:power",
        "power": "growver:blacklisted",
        "inverted": true
    }
}